{
  "name": "@photogate/api",
  "version": "0.0.1-alpha0",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "types": "./lib/types",
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "prepare": "rm -rf dist && npm run build",
    "build": "rm -rf dist && microbundle --format cjs,esm --target node",
    "build-local": "npm run build && cp $(npm pack | tail -n 1) ~",
    "dev": "microbundle --sourcemap=true --target node --format cjs,esm watch",
    "test": "jest ./src",
    "test:watch": "jest ./src --watchAll",
    "lint": "eslint ./src/**/* --ext .js,.jsx",
    "serve:coverage": "cd ./coverage/lcov-report && python3 -m http.server 5544"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@types/color": "^3.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/mime": "^3.0.1",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^18.16.17",
    "@types/request-promise": "^4.1.51",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.42.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "rollup": "^2.79.1",
    "source-map-support": "^0.5.21",
    "typescript": "^5.1.3",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "@colour-extractor/colour-extractor": "^1.0.1",
    "better-sqlite3": "^10.0.0",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "color": "^4.2.3",
    "color.js": "^1.2.0",
    "colors": "^1.4.0",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dominant-color": "^0.0.1",
    "dotenv": "^16.0.3",
    "exifremove": "^1.0.1",
    "exiftool-vendored": "^25.2.0",
    "express": "^4.19.2",
    "filenamify": "^6.0.0",
    "heic-convert": "^2.1.0",
    "heic2any": "^0.0.4",
    "jest": "^29.5.0",
    "knex": "^2.5.1",
    "lodash": "^4.17.21",
    "microbundle": "^0.15.1",
    "mime": "^3.0.0",
    "moment": "^2.29.4",
    "native-querystring": "^1.1.1",
    "node-exiftool": "^2.3.0",
    "redis": "^4.6.13",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "sharp": "^0.33.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "util": "^0.12.5"
  }
