{
	"name": "@photogate/media-exif-sdk",
	"version": "0.0.1-alpha0",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"types": "./lib/types",
		"require": "./dist/index.cjs",
		"default": "./dist/index.modern.js"
	},
	"module": "./dist/index.module.js",
	"unpkg": "./dist/index.umd.js",
	"scripts": {
		"prepare": "rm -rf dist && npm run build",
		"build": "rm -rf dist && microbundle",
		"build-local": "npm run build && cp $(npm pack | tail -n 1) ~",
		"dev": "microbundle watch",
		"test": "jest ./src",
		"test:watch": "jest ./src --watchAll",
		"lint": "eslint ./src/**/* --ext .js,.jsx",
		"serve:coverage": "cd ./coverage/lcov-report && python3 -m http.server 5544"
	},
	"devDependencies": {
		"@jest/globals": "^29.5.0",
		"@types/color": "^3.0.3",
		"@types/jest": "^29.5.0",
		"@types/mime": "^3.0.1",
		"@types/node": "^18.16.17",
		"@typescript-eslint/eslint-plugin": "^5.57.0",
		"@typescript-eslint/parser": "^5.57.0",
		"eslint": "^8.42.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^5.0.1",
		"rollup": "^2.79.1",
		"typescript": "^5.1.3",
		"uglify-es": "^3.3.9"
	},
	"dependencies": {
		"@colour-extractor/colour-extractor": "^1.0.1",
		"@photogate/types": "file:../types/photogate-types-1.0.0.tgz",
		"chalk": "^4.1.2",
		"color": "^4.2.3",
		"color.js": "^1.2.0",
		"colors": "^1.4.0",
		"dominant-color": "^0.0.1",
		"dotenv": "^16.0.3",
		"exifremove": "^1.0.1",
		"jest": "^29.5.0",
		"microbundle": "^0.15.1",
		"mime": "^3.0.0",
		"moment": "^2.29.4",
		"native-querystring": "^1.1.1",
		"node-exiftool": "^2.3.0",
		"sharp": "^0.32.1",
		"ts-jest": "^29.1.0",
		"util": "^0.12.5"
	}
}
